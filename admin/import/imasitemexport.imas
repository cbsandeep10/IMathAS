$ssGM=sumarray(calconarray($GMarray,"x^2"))
$ssTreatment=3*$Neffect^2+3*$Feffect^2+3*$Beffect^2+3*$Ceffect^2
$ssError=sumarray(calconarray([$e11,$e12,$e13,$e21,$e22,$e23,$e31,$e32,$e33,$e41,$e42,$e43],"x^2"))

//Calculate Mean Squares
$msTreatment=$ssTreatment / $dfTreatment
$msError=$ssError / $dfError

//Calcuate the F Ratio
$fRatio=$msTreatment /$msError

//Define Answer Array for Screens
$answer=[$GM,$GM,$GM,$GM,$GM,$GM,$GM,$GM,$GM,$GM,$GM,$GM,$Neffect,$Feffect,$Beffect,$Ceffect,$Neffect,$Feffect,$Beffect,$Ceffect,$Neffect,$Feffect,$Beffect,$Ceffect,$e11,$e21,$e31,$e41,$e12,$e22,$e32,$e42,$e13,$e23,$e33,$e43]

