$e32=$Beet[1]-$GM-$Beffect
$e33=$Beet[2]-$GM-$Beffect
$e41=$Ctl[0]-$GM-$Ceffect
$e42=$Ctl[1]-$GM-$Ceffect
$e43=$Ctl[2]-$GM-$Ceffect

//Calculate Degrees of Freedom
$dfTotal=count($full)
