$df1=showrecttable($data,["V1","V2","V3","V4"],["F1","F2","F3","F4"],"c")

//Define answers based on data
//Get Grand Mean
$full=mergearrays($field1,mergearrays($field2,mergearrays($field3,$field4)))
$GM=mean($full)
$GMarray=fillarray($GM,16)

//Calculate Block Effects
$f1Effect=mean(calconarray($field1,"x-$GM"))
$f2Effect=mean(calconarray($field2,"x-$GM"))
$f3Effect=mean(calconarray($field3,"x-$GM"))
$f4Effect=mean(calconarray($field4,"x-$GM"))
$Barray=mergearrays(fillarray($f1Effect,4),mergearrays(fillarray($f2Effect,4),mergearrays(fillarray($f3Effect,4),fillarray($f4Effect,4))))

//Calculate Treatment Effects
$v1Effect=mean([$variety1[0]-$GM-$f1Effect,$variety1[1]-$GM-$f2Effect,$variety1[2]-$GM-$f3Effect,$variety1[3]-$GM-$f4Effect])
$v2Effect=mean([$variety2[0]-$GM-$f1Effect,$variety2[1]-$GM-$f2Effect,$variety2[2]-$GM-$f3Effect,$variety2[3]-$GM-$f4Effect])
$v3Effect=mean([$variety3[0]-$GM-$f1Effect,$variety3[1]-$GM-$f2Effect,$variety3[2]-$GM-$f3Effect,$variety3[3]-$GM-$f4Effect])
$v4Effect=mean([$variety4[0]-$GM-$f1Effect,$variety4[1]-$GM-$f2Effect,$variety4[2]-$GM-$f3Effect,$variety4[3]-$GM-$f4Effect])
$Tarray=[$v1Effect,$v2Effect,$v3Effect,$v4Effect,$v1Effect,$v2Effect,$v3Effect,$v4Effect,$v1Effect,$v2Effect,$v3Effect,$v4Effect,$v1Effect,$v2Effect,$v3Effect,$v4Effect]

//Calculate Errors
$X=matrix($full,4,4)
$mu=matrix($GMarray,4,4)
$beta=matrix($Barray,4,4)
$alpha=matrix($Tarray,4,4)
$epsilon=matrixdiff(matrixdiff(matrixdiff($X,$mu),$beta),$alpha)
$e11=matrixgetentry($epsilon,0,0)
$e12=matrixgetentry($epsilon,0,1)
$e13=matrixgetentry($epsilon,0,2)
$e14=matrixgetentry($epsilon,0,3)
$e21=matrixgetentry($epsilon,1,0)
$e22=matrixgetentry($epsilon,1,1)
$e23=matrixgetentry($epsilon,1,2)
$e24=matrixgetentry($epsilon,1,3)
$e31=matrixgetentry($epsilon,2,0)
$e32=matrixgetentry($epsilon,2,1)
$e33=matrixgetentry($epsilon,2,2)
$e34=matrixgetentry($epsilon,2,3)
$e41=matrixgetentry($epsilon,3,0)
$e42=matrixgetentry($epsilon,3,1)
$e43=matrixgetentry($epsilon,3,2)
$e44=matrixgetentry($epsilon,3,3)

//Calculate Degrees of Freedom
$dfTotal=count($full)
$dfGM=1
$dfBlock=4-1
$dfTreatment=4-1
$dfError=$dfTotal-$dfGM-$dfBlock-$dfTreatment

//Calculate Sums of Squares
$ssTotal=sumarray(calconarray($full,"x^2"))
