$answer[38]=$dfTreatment
$answer[39]=$dfError
$answer[40]=$ssTotal
$answer[41]=$ssGM
$answer[42]=$ssTreatment
$answer[43]=$ssError
$answer[44]=$msTreatment
$answer[45]=$msError
$answer[46]=$fRatio
